---
import Base from "../layouts/Base.astro";
import HeroSection from "../sections/Hero";
import PlanOutline from "../sections/PlanOutline";
import CTA from "../sections/CTA";
import Cases from "../sections/Cases";

import { navigation, siteData } from "../consts";

---
<Base {navigation} {siteData}>
  <HeroSection {...siteData} />
  <PlanOutline problem={siteData.problem} steps={siteData.steps}/>
  <CTA {...siteData.cta} />
  <Cases cases={siteData.cases} />
</Base>
