---
import Base from "../layouts/Base.astro";
import HeroSection from "../sections/Hero.astro";
import PlanOutline from "../sections/PlanOutline.astro";
import CTA from "../sections/CTA.astro";
import Cases from "../sections/Cases.astro";

import { getCollection } from 'astro:content';

const cases  = (await getCollection('cases'))
import { navigation, siteData } from "../consts";
---
<Base {navigation} {siteData}>
  <HeroSection {...siteData} />
  <PlanOutline {...siteData.process} />
  <CTA {...siteData.cta} />
  <Cases {...siteData.work} {cases} />
</Base>
