---
import BaseHead from "../components/BaseHead.astro";
import Footer from "../sections/Footer.astro";
import Header from "../components/Header"

const { navigation, siteData } = Astro.props;
---


<html lang="en">
	<head>
    <BaseHead {...siteData} />
	</head>
	<body>
    <Header navigation={navigation} client:load/>
    <slot/>
    <Footer {navigation} />
	</body>
</html>
